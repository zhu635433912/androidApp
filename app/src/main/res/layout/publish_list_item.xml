<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#fdfdfd"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <View
        android:id="@+id/text23"
        android:background="#e6e6e6"
        android:layout_width="match_parent"
        android:layout_height="15px"/>
    <RelativeLayout
        android:layout_below="@+id/text23"
        android:layout_marginTop="20px"
        android:layout_gravity="center"
        android:layout_width="700px"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        >

        <com.facebook.drawee.view.SimpleDraweeView
            app:roundAsCircle="true"
            android:id="@+id/lognhost"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="14px"
            android:scaleType="centerInside"
            android:layout_marginTop="10px"
            app:placeholderImage="@drawable/ox2"
            android:layout_width="0px"
            android:layout_height="80px" />
        <TextView
            android:layout_marginLeft="25px"
            android:layout_marginTop="16px"
            android:layout_alignTop="@+id/lognhost"
            android:layout_toRightOf="@+id/lognhost"
            android:textSize="30px"
            android:id="@+id/text1"
            android:text="名字"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <com.facebook.drawee.view.SimpleDraweeView
            android:layout_width="50px"
            android:layout_marginTop="20px"
            android:layout_marginRight="60px"
            app:roundAsCircle="true"
            android:layout_toLeftOf="@+id/text3"
            android:id="@+id/teacher_head_image1"
            android:layout_height="50px" />
        <com.facebook.drawee.view.SimpleDraweeView
            android:layout_width="50px"
            app:roundAsCircle="true"
            android:layout_marginTop="20px"
            android:layout_marginRight="10px"
            android:layout_toLeftOf="@+id/teacher_head_image1"
            android:id="@+id/teacher_head_image2"
            android:layout_height="50px" />
        <com.facebook.drawee.view.SimpleDraweeView
            android:layout_width="50px"
            app:roundAsCircle="true"
            android:layout_marginTop="20px"
            android:layout_marginRight="10px"
            android:layout_toLeftOf="@+id/teacher_head_image2"
            android:id="@+id/teacher_head_image3"
            android:layout_height="50px" />
        <TextView
            android:background="@mipmap/sure_complete_bg"
            android:id="@+id/text3"
            android:gravity="center"
            android:layout_marginRight="25px"
            android:layout_marginTop="20px"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:textColor="#f43232"
            android:textSize="30px"
            android:text="接取信息"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/tlucahwodjawf"
            android:layout_marginTop="30px"
            android:layout_marginLeft="120px"
            android:background="#fbb805"
            android:layout_below="@+id/text3"
            android:layout_width="match_parent"
            android:layout_height="1px" />
        <TextView
            android:layout_marginLeft="20px"
            android:textColor="#4b4b4b"
            android:textSize="40px"
            android:layout_below="@+id/tlucahwodjawf"
            android:layout_alignParentLeft="true"
            android:id="@+id/text4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <TextView
            android:layout_marginTop="16px"

            android:layout_marginRight="20px"
            android:layout_alignTop="@+id/text4"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="20px"
            android:id="@+id/text2"
            android:textSize="24px"
            android:textColor="#2dd0fd"
            android:text="年级"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:layout_marginTop="16px"
            android:textColor="#fbb805"
            android:textSize="25px"
            android:text=""
            android:layout_toLeftOf="@+id/text2"
            android:layout_alignTop="@+id/text4"
            android:layout_marginRight="10px"
            android:id="@+id/education_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:layout_marginRight="50px"
            android:layout_marginTop="16px"
            android:layout_alignTop="@+id/text4"
            android:layout_toRightOf="@+id/text2"
            android:textColor="#383738"
            android:id="@+id/text9"
            android:textSize="24px"
            android:text="服务类型"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />



        <TextView
            android:layout_marginTop="20px"
            android:layout_marginLeft="20px"
            android:textColor="#4b4b4b"
            android:textSize="33px"
            android:text=""
            android:layout_below="@+id/tlucahwodjawf"
            android:layout_toRightOf="@+id/text4"
            android:id="@+id/text5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <TextView
            android:textSize="28px"
            android:id="@+id/text6"
            android:maxLines="2"
            android:text="u回顾会UI刽 u付uUI与已 吞吞吐吐拖拖拖拖拖吞吞吐吐拖拖拖拖拖吞吞吐吐拖拖 吞吞吐吐拖拖拖拖拖 通天塔"
            android:layout_width="570px"
            android:layout_height="wrap_content"
            android:layout_below="@+id/text5"
            android:layout_toRightOf="@+id/teacher_address_icon"
            />
        <!--android:layout_toEndOf="@+id/teacher_address_icon" />-->
        <ImageView
            android:layout_below="@+id/text6"
            android:id="@+id/teacher_address_icon"
            android:layout_marginLeft="14px"
            android:layout_marginTop="20px"
            android:layout_alignParentLeft="true"
            android:src="@mipmap/main_address_icon"
            android:layout_width="0px"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/text7"
            android:layout_below="@+id/text6"
            android:layout_marginLeft="14px"
            android:layout_marginTop="20px"
            android:layout_marginBottom="20px"
            android:layout_toRightOf="@+id/teacher_address_icon"
            android:text="14km"
            android:textSize="20px"
            android:layout_width="0px"
            android:layout_height="0px" />
        <TextView
            android:id="@+id/text8"
            android:layout_marginRight="14px"
            android:layout_marginBottom="20px"
            android:layout_marginTop="20px"
            android:layout_alignTop="@+id/text4"
            android:layout_alignParentRight="true"
            android:textSize="20px"
            android:text="11111"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <ImageView
            android:layout_below="@+id/text6"
            android:layout_marginRight="20px"
            android:layout_toLeftOf="@+id/text8"
            android:layout_marginTop="30px"
            android:id="@+id/imagehaop"
            android:layout_width="0px"
            android:layout_height="20px" />
        <ImageView
            android:id="@+id/complete_image"
            android:layout_marginRight="50px"
            android:layout_centerInParent="true"
            android:layout_marginTop="100px"
            android:layout_width="150px"
            android:src="@mipmap/complete_icon"
            android:layout_height="150px" />
    </RelativeLayout>

</RelativeLayout>