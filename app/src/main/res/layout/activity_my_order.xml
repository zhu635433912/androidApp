<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.deguan.xuelema.androidapp.MyOrderActivity">
    <RelativeLayout
        android:id="@+id/stuentordentfanhui"
        android:layout_width="100px"
        android:layout_height="80px"></RelativeLayout>
    <ImageButton
        android:layout_marginLeft="30px"
        android:layout_marginTop="20px"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:background="@drawable/add19"
        android:layout_width="20px"
        android:layout_height="30px" />

    <TextView
        android:textSize="35px"
        android:layout_marginTop="20px"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="我的订单"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/xuqiufenggexian"
        android:background="#d7d7d7"
        android:layout_marginTop="80px"
        android:layout_below="@+id/neirongtext"
        android:layout_width="match_parent"
        android:layout_height="1px" />
    <android.support.design.widget.TabLayout
        android:id="@+id/my_order_tablayout"
        app:tabTextColor="#8b8b8b"
        app:tabIndicatorColor="#fd1245"
        app:tabSelectedTextColor="#fd1245"
        android:layout_width="match_parent"
        android:layout_height="100px"/>
    <android.support.v4.view.ViewPager
        android:id="@+id/my_order_vp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</LinearLayout>
