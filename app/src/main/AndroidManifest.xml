        <activity android:name=".Distribution_Activty" android:launchMode="singleTask"/>
        <activity android:name=".Promote_Acitvty" android:launchMode="singleTask"/>
        <activity android:name=".MyDistribution_Actvity" android:launchMode="singleTask"/>
        <activity android:name=".Extended_Activty" android:launchMode="singleTask"/>
        <activity android:name=".Start_Activty" android:launchMode="singleTask" />
        <activity android:name=".Closing" android:launchMode="singleTask" />
